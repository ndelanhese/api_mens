    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "fs": "^0.0.1-security",
    "joi": "^17.7.0",
    "joi-translation-pt-br": "^0.0.6",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.0",
    "pg": "^8.7.3",
    "redis": "^4.6.4",
    "sequelize": "^6.28.1",
    "sequelize-cli": "^6.4.1",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
