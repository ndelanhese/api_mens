{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint": "npx eslint . --ext .js,.ts,",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed": "npx sequelize-cli db:seed",
    "db:setup": "npm run db:create && npm run db:migrate && npm run db:seed:all",
    "rm:dist": "rm -rf dist",
    "db:reset": "npm run db:drop && npm run db:setup",
    "build": "npm run rm:dist && npx tsc && tsc-alias && cp -r ./app/database/Migrations ./dist/database/Migrations",
    "start": "node ./dist/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./app/server.ts",
    "test": "jest",
    "coverage": "npx tsc && jest ./dist --coverage",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prettier": "prettier ./app --write",
    "format": "npm run prettier && npm run lint:fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
